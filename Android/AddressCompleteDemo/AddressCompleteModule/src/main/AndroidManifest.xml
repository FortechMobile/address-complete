<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ro.fortech.addresscompletedemo" >

    <!-- Request Internet permissions -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ro.fortech.addresscompletedemo.ActivityAddressComplete"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ro.fortech.addresscompletedemo.ActivityVendorDetails"
            android:label="@string/title_activity_activity_vendor_details" >
        </activity>
    </application>

</manifest>
